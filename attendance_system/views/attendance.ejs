<body>
  <header>
    <nav>
     <div class="nav-wrapper container">
       <a href="#" class="brand-logo center main-title">GURU GOBIND SINGH INDRAPRASTHA UNIVERSITY</a>
     </div>
   </nav>


   <nav>
    <div class="nav-wrapper sub-nav">
      <a href="#" class="brand-logo center sub-title">FACULTY FEEDBACK SYSTEM</a>
    </div>
  </nav>
</header><main>

<div class="container drop-down-school">
  <div class="row login-sel-cont">
    <div class="col-md-4 col-md-offset-4">
    <form action = "users" method = "post">
      <!-- <p>Date: <input type="text" name="dateofattendance" id="datepicker" style="border:solid;"></p> -->
      Date: <input type="date" class="datepicker" name="dateofattendance">
      
      <div class="input-field">
        Teaching Hours:
        <select name="teachinghours" id="number">
          <option value="1" selected="selected">1</option>
          <option value="2">2</option>
          <option value="3">3</option>
          <option value="4">4</option>
        </select>
        <!-- <label>Materialize Select</label> -->
        <!-- <label for="number">Teaching Hours</label> -->
      </div>
      <!-- <select name="teachinghours" id="number">
      <option selected="selected">1</option>
      <option>2</option>
      <option>3</option>
      <option>4</option>
      </select> -->
      <table>
         <tr>
            <th>enroll</th><th>attendance</th>
         </tr>

         <% for (var i = 0; i < data.length;i++) { %>
          <tr>
            <td><%= data[i].enroll_no %></td>
            <td>                        
              <input type="checkbox" name='<%= data[i].enroll_no %>' id='<%= data[i].enroll_no %>'/>
              <label for='<%= data[i].enroll_no %>'></label> 
            </td>
          </tr>    
         <% } %>
      </table>

      <button class="btn waves-effect waves-light" type="submit" name="action">Submit
        <i class="material-icons right">send</i>
      </button>
        
    </form>

</div>
</div>

</div></main>

